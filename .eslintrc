{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-const-assign": "error",
        "no-undef": "error",
        "no-var": "error",
        "react/prop-types": 0,
        "no-empty": [
            "error",
            {
                "allowEmptyCatch": true
            }
        ]
    }
}